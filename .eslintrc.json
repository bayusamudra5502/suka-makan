{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    },
    "settings": {
        "import/resolver":{
            "alias":{
                "map" : [
                    ["@img","./src/images"],
                    ["@style","./src/styles"],
                    ["@","./src"]
                ]
            }
        }
    }
}
